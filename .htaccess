RewriteEngine on
RewriteCond %{HTTP_HOST} ^www\.stellarsecurity\.com [NC]
RewriteRule ^(.*)$ https://stellarsecurity.com/$1 [L,R=301]
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]

RewriteRule ^(.*) /index.html [NC,L]